
PHONY: objects

libcabbage.a cabbage.elf cabbage.bin: $(BUILD_DIR) 
	$(MAKE) -C $(BUILD_DIR) -f $(PWD)/makefiles/build.gmake -I$(PWD)/makefiles/ $@ TOP_DIR="$(PWD)" SOURCE_DIR="$(PWD)" SOURCE_SUBDIRS="$(SOURCE_SUBDIRS)"


objects: $(BUILD_DIR)
	$(MAKE) -C $(BUILD_DIR) -f $(PWD)/makefiles/compile.gmake -I$(PWD)/makefiles/ SOURCE_DIR="$(PWD)" SOURCE_SUBDIRS="$(SOURCE_SUBDIRS)"

$(BUILD_DIR):
	mkdir -p $(BUILD_DIR)

.DEFAULT_GOAL :=

