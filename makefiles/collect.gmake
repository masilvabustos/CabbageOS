
include last-makefile.defs

include db.defs

list-sources 	=$(foreach suffix,.c .s .S,$(wildcard $(1)/*$(suffix)))

PARENT_MAKEFILE =$(word 1,$(subst $(MAKEFILES),,$(MAKEFILE_LIST)))

get-objects 	=$(subst $(abspath $(SOURCE_DIR)),,$(abspath $(patsubst %.c,%.o,$(call list-sources,$(1)))))

list-subs = $(foreach subdir,$(1),$(LAST_MAKEFILE_DIR)/$(subdir)/objects.gmake)

define COLLECT_OBJECTS

OBJECTS += $(call get-objects,$(LAST_MAKEFILE_DIR))

end

OBJECTS :=

